package user

import (
	"github.com/shivambaku/go-web-templ-htmx-tailwind-demo/views/layouts"
	"github.com/shivambaku/go-web-templ-htmx-tailwind-demo/models"
)

script click(user model.User) {
	alert("Hello, " + user.Email + "!")
}

templ Show(user model.User) {
	@layout.Default() {
		<h1>Hello, { user.Email } !</h1>
		<button onclick={ click(user) }>Click me!</button>
	}
}
